    /**
     * Handles all the responses.
     *
     * @param test the response from the server
     * @param after alert where the flow is going
     * @param before the form that was before this screen
     */
    private void handleResponse(WSResponse test, Alert after, Form before) {

        StringBuffer buff = new StringBuffer();        
        alertServer.setTimeout(1500);
        alertServer.setType(AlertType.INFO);
        switchDisplayable(null, alertServer);
        logme.info("[handleResponse]alertServer is shown: "+alertServer.isShown());

        if (handleResponseWithCare(test)) {
          

            logme.info("[UNPARSABLE]Connection broken or server down!");
            //Alert for unparsable error : come back to the previous screen
            buff.append(LocalizationSupport.getMessage("CONNECTION_ERROR")).append("\n");
            buff.append(LocalizationSupport.getMessage("CONNECTION_ERROR2"));
            alertCare.setString(buff.toString());
            alertCare.setTitle("");
            alertCare.setTimeout(2000);
            alertCare.setType(AlertType.ERROR);
            //alertCare.setImage(getImage3());
            logme.info("[UNPARSABLE HANDLE]alertCare" + before.getTitle());
            switchDisplayable(alertCare, before);

        } else if (test.getErrorCode() == Constants.ERROR_NONE) {

            //normal scenario
            logme.info("[NORMAL_SCENARIO] after handle: " + controller.getResponse());
            printList = test.getResponseTable();

            logme.info("[NORMAL_SCENARIO]OK:result before printing ");
            Utilz.readHashtable(printList);
            logme.info("[NORMAL HANDLE]" + after.getTitle());

            // this will get through

            switch (controller.getSid()) {
                case Constants.CREATE_CUSTOMER_ID:
                    alertServer.setTimeout(1200);
                    alertServer.setString(Constants.CREATE_CUSTOMER_TAG);
                    switchDisplayable(null, getCreateCustAccPrintPreview());
                    break;
                case Constants.CREDIT_CUSTOMER_ID:
                    alertServer.setTimeout(1200);
                    alertServer.setString(Constants.CREDIT_CUSTOMER_TAG);
                    switchDisplayable(null, getCreditCustAccPrintPreview());
                    break;
                case Constants.TOP_UP_CASH_ID:
                    alertServer.setString(Constants.TOP_UP_CASH_TAG);
                    alertServer.setTimeout(1200);
                    switchDisplayable(null, getTopUpCashPayPreview());
                    break;
                case Constants.TOP_UP_WALLET_ID:
                    alertServer.setTimeout(1200);
                    alertServer.setString(Constants.TOP_UP_WALLET_TAG);
                    switchDisplayable(null, getTopUpmPayPreview());
                    break;
                case Constants.E_VOUCHER_CASH_ID:
                    alertServer.setTimeout(1200);
                    alertServer.setString(Constants.E_VOUCHER_CASH_TAG);
                    switchDisplayable(null, getEVoucherCashPrintPreview());
                    break;
                case Constants.E_VOUCHER_WALLET_ID:
                    alertServer.setTimeout(1200);
                    alertServer.setString(Constants.E_VOUCHER_WALLET_TAG);
                    switchDisplayable(null, getEVoucherMPaymentPrintPreview());
                    break;
                default:
                    logme.info("Nowhere to go");
                    // switchDisplayable(null, getAlertPrint());
                    break;
            }

            // write post-action user code here
        } else {
            //error is parsable : must be usage error
            StringBuffer tmp = new StringBuffer();
            
            logme.info("IS SHOWN"+before.isShown());
          
            printList = test.getResponseTable();
            Utilz.readHashtable(printList);

            alertCare.setType(AlertType.ERROR);
           // alertCare.setImage(getImage3());
            alertCare.setTitle(LocalizationSupport.getMessage("USAGE_ERROR") + "#" + test.getErrorCode());
            alertCare.setString(LocalizationSupport.getMessage("ERROR_MESSAGE") + printList.get("errorCode") + "\n" + printList.get("errorMessage"));
            alertCare.setTimeout(2000);
            tmp.append(LocalizationSupport.getMessage("ERROR_MESSAGE")).append(printList.get("errorCode"));
           
            logme.info("[PARSABLE]where to go "+before.getTitle());
            
            //waitForTopUpCash.getTask().notifyAll();
            //switchDisplayable(alertCare, before);
            
             switch (controller.getSid()) {
                case Constants.CREATE_CUSTOMER_ID:
                    switchDisplayable(alertCare, getCreateCustomerAccount());
                    break;
                case Constants.CREDIT_CUSTOMER_ID:
                    switchDisplayable(alertCare, getCreditCustomerAccount());
                    break;
                case Constants.TOP_UP_CASH_ID:
                    switchDisplayable(alertCare, getTopUpCashCredit());
                    break;
                case Constants.TOP_UP_WALLET_ID:
                    switchDisplayable(alertCare, getTopUpmPay());
                    break;
                case Constants.E_VOUCHER_CASH_ID:
                    switchDisplayable(alertCare, getEVoucherCash());
                    break;
                case Constants.E_VOUCHER_WALLET_ID:
                    switchDisplayable(alertCare, getEVoucherMPay());
                    break;
                default:
                    logme.info("Nowhere to go");
                    // switchDisplayable(null, getAlertPrint());
                    break;
            }
            

        }
    }

    
    /**
     * Get the selected voucher
     *
     * @return a string with selected voucher
     */
    private String getSelectedVoucher(ChoiceGroup cg) {

        //looking in the choice item for the chosen one
        for (int i = 0; i < cg.size(); i++) {
            if (cg.isSelected(i)) {
                return cg.getString(i);
            }
        }
        return null;
    }

    /**
     * Read the baseURL always from repository
     *
     * @return baseURL from repository
     */
    private String getBaseURL() {
        StringBuffer sb = new StringBuffer();
        //re-read the config
        mobiConfig.read();
        sb.append(mobiConfig.getConn().getProtocol());
        sb.append(ConfigUtils.SEMICOLON).append(ConfigUtils.DOUBLE_SLASH);
        sb.append(mobiConfig.getConn().getHost()).append(ConfigUtils.SEMICOLON);
        sb.append(mobiConfig.getConn().getPort());
        return sb.toString();
    }

    /**
     * Starting a connection in separate thread.
     */
    public void run() {
        HttpConnection hc = null;
        try {
            hc = (HttpConnection) Connector.open("http://www.google.it");
            int rc = hc.getResponseCode();
            logme.info("[googleit]Open connection with response code:" + rc);
          
                hc.setRequestMethod(HttpConnection.GET);
                logme.info("[googleit]get the connection");
            
        } catch (IOException ex) {
            logme.info("[googleit]exception " + ex.getMessage());
        } catch (Throwable e) {

            logme.error("[googleit]ERROR while sending! " + e.getMessage());

        } finally {
            if (hc != null) {
                try {
                    hc.close();
                } catch (IOException ex) {
                    logme.error("[googleit] cannot close connection! " + ex.getMessage());
                }
            }
        }
    }

