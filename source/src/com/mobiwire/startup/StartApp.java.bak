    /**
     * Performs an action assigned to the methodCheckSettings entry-point.
     */
    public void methodCheckSettings() {//GEN-END:|617-entry|0|618-preAction
        // write pre-action user code here


        controller.sendRequest(Constants.CREATE_CUSTOMER_ID);

        synchronized (controller) {

            while (!controller.getResponse().isErrorSet()) {
            }
            for (int i = 0; i < 1; i++) {
                try {
                    controller.wait();
                } catch (InterruptedException ex) {
                    ex.printStackTrace();
                }
            }
        }
        
        WSResponse test = controller.getResponse();
  
        Utilz.readHashtable(test.getResponseTable());
        logme.info("[CHECK_SETTINGS]error#"+test.getErrorCode());
        
        StringBuffer buff = new StringBuffer();
        // if the settings are wrong 
        if (handleResponseWithCare(test)) {

            //Alert for unparsable error : come back to the previous screen
            logme.info("[SETTINGS]Server KO +error#"+test.getErrorCode());
            
            buff.append(LocalizationSupport.getMessage("SETTINGS_SERVER_NOT1")).append("\n");
            buff.append(LocalizationSupport.getMessage("SETTINGS_SERVER_NOT2"));
            alertPingu.setString(buff.toString());
            alertPingu.setTimeout(2000);
            alertPingu.setType(AlertType.ERROR);
            
            //don't save and come back
             logme.info("[SETTINGS]will not save");
            
             switchDisplayable(alertPingu, getSettingsScreen());
            
            footerSettings.setText(LocalizationSupport.getMessage("SETTINGS_FAILED"));

        } else {
            //we are expecting a parsable error in the case the settings are good
            //we can see the results
            printList = test.getResponseTable();
            logme.info("[SETTINGS]parsable error: Mobiprint server is there");
            Utilz.readHashtable(printList);
             
            alertPingu.setType(AlertType.INFO);
            alertPingu.setString(LocalizationSupport.getMessage("SETTINGS_SAVED"));
            alertPingu.setTimeout(2000);
            
            
            //what to save
            mobiConfig.getConn().setPort(portSettings.getString());
            
            if ((protocolChoice.getSelectedIndex() == 0)) {
                mobiConfig.getConn().setProtocol("http");
            }
            if ((protocolChoice.getSelectedIndex() == 1)) {
                mobiConfig.getConn().setProtocol("https");
            }

            mobiConfig.getConn().setHost(hostSettings.getString());
            
            mobiConfig.write();//write only when is ok
            // end save

            
            footerSettings.setText(LocalizationSupport.getMessage("SETTINGS_SAVED"));
            
            logme.info("[SAVED]reading from repository");
            logme.info("reading from repository");
            logme.info("protocol " + mobiConfig.getConn().getProtocol());
            logme.info("host " + mobiConfig.getConn().getHost());
            logme.info("port " + mobiConfig.getConn().getPort());
            
            //switch back anyway
            switchDisplayable(alertPingu, getSettingsScreen());
            
        }
//GEN-LINE:|617-entry|1|618-postAction
        // write post-action user code here



    }//GEN-BEGIN:|617-entry|2|
//</editor-fold>//GEN-END:|617-entry|2|

